filter(type=="sample") %>%
ggplot(aes(x=Day, y=Area, color=Measurement)) +
geom_point() +
facet_wrap(~Measurement) +
theme_test()
co2_raw %>%
filter(type=="sample") %>%
ggplot(aes(x=Day, y=Area, color=Measurement)) +
geom_point() +
theme_test()
co2_raw %>%
filter(type=="sample" & Measurement=="C13") %>%
ggplot(aes(x=Day, y=Area, color=Sample)) +
